    <em:localized>
      <Description>
        <em:locale>de</em:locale>
        <em:description>Vim Tastenbefehle für Firefox</em:description>
      </Description>
    </em:localized>

	<em:localized>
      <Description>
        <em:locale>el-GR</em:locale>
        <em:description>Συντομεύσεις Πληκτρολογίου του Vim για Firefox</em:description>
      </Description>
    </em:localized>

    <em:localized>
      <Description>
        <em:locale>nl</em:locale>
        <em:description>Vim sneltoetsen voor Firefox</em:description>
      </Description>
    </em:localized>

    <em:localized>
      <Description>
        <em:locale>id</em:locale>
